				<%= form_with(model: @city_introductio, local: true) do |form| %>

									  <div class="場所">
												<%= form.label :場所 %>
												<%= form.text_field :name %>


									<div class="時間">
												<%= form.label :時間 %>
												<%= form.datetime_select :start_time %>
									</div>

									<div class="コメント">
												<%= form.label :内容 %>
												<%= form.text_field :comment %>
									</div>


									<div class="submit">
										  <%= form.submit %>
								
						</div>
				<% end %>